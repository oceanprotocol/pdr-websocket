{
  "name": "express-typescript-ethersjs",
  "version": "1.0.0",
  "description": "",
  "main": "src/app.ts",
  "scripts": {
    "start": "env-cmd -f ./.env.local nodemon --watch 'src/**/*.ts' --exec ts-node src/app.ts",
    "staging": "env-cmd -f ./.env.staging nodemon --watch 'src/**/*.ts' --exec ts-node src/app.ts",
    "production": "env-cmd -f ./.env.production nodemon --watch 'src/**/*.ts' --exec ts-node src/app.ts",
    "create": "plop",
    "build-dev": "tsc -p tsconfig.json",
    "start-dev": "NODE_ENV=dev node build/app.js",
    "build-production": "tsc -p tsconfig.json",
    "start-production": "NODE_ENV=production node build/app.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@apollo/client": "^3.7.16",
    "cors": "^2.8.5",
    "cross-fetch": "^3.1.8",
    "dotenv": "^16.3.1",
    "ethers": "^5.7.2",
    "express": "^4.18.2",
    "graphql": "^16.8.1",
    "socket.io": "^4.6.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.3.1",
    "env-cmd": "^10.1.0",
    "nodemon": "^2.0.22",
    "plop": "^3.1.2"
  }
}
